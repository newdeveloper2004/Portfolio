@model IEnumerable<Members>
    @{
        ViewData["Title"] = "Members";
    }

    <div class="container p-3">
        <div class="row pt-4">
            <div class="col-6">
                <h2>Members List</h2>
            </div>
            <div class="col-6 text-end">
                <a asp-controller="Book" asp-action="Join" class="btn btn-primary">
                    Add New Member
                </a>
            </div>
        </div>
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    ID
                </th>
                <th>
                   Name
                </th>

            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td>
                        @obj.Id
                    </td>
                    <td>
                        @obj.Name
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
